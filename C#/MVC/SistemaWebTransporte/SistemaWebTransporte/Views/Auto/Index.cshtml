@model IEnumerable<SistemaWebTransporte.Models.Auto>
@{
    ViewBag.Title = "Index";

}

<h2>Index</h2>

@Html.ActionLink("Nuevo", "Create", "Auto")

<table>
    <tr>
        <th>@Html.DisplayNameFor(model => model.Modelo)</th>
        <th>@Html.DisplayNameFor(model => model.Marca)</th>
        <th>@Html.DisplayNameFor(model => model.Anio)</th>
        <th>@Html.DisplayNameFor(model => model.Color)</th>

    </tr>

    @foreach (var item in Model)
    {
<tr>
    <td>@Html.DisplayFor(modelItem => item.Modelo)</td>
    <td>@Html.DisplayFor(modelItem => item.Marca)</td>
    <td>@Html.DisplayFor(modelItem => item.Anio)</td>
    <td>@Html.DisplayFor(modelItem => item.Color)</td>
    <td>@Html.ActionLink("Details", "Details", new { id = item.Id })</td>
    <td>@Html.ActionLink("Delete", "Delete", new { id = item.Id }) </td>
</tr>

}


</table>




